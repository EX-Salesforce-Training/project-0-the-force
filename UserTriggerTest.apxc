        Profile s = [SELECT Id FROM Profile WHERE Name='Standard User'];
        
        for (integer i = 0; i<5; i++){
          User u = new User(Alias = 'User' + i, 
                   Email= 'User' + i + '@cunning-fox-wrr40n.com', 
                   EmailEncodingKey='UTF-8', 
                   LastName='Testing', 
                   LanguageLocaleKey='en_US', 
